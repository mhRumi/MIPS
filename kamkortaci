
.text
.globl main

main:
	li $v0 4   #msg1 printing
	la $a0, msg1
	syscall
	
	# input A
	li $v0 5
	syscall
	move $t0, $v0
	
	#msg2 printing
	li $v0 4
	la $a0, msg2
	syscall
	
	#input B
	
	li $v0 5
	syscall
	move $t1,  $v0
	
	#add
	add $t1 $t0, $t1
	#for print int
	
	li $v0 4
	la $a0 , msg3
	syscall
	
	li $v0 1
	move $a0, $t1
	syscall
	
	.data
msg1: .asciiz "Enter A:  "
msg2: .asciiz "Enter B:  "
msg3: .asciiz  "A + B =   "

